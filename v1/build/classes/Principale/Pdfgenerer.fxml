<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="700.0" prefWidth="1370.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Fxml.PdfgenererController">
   <children>
      <ComboBox fx:id="combobp" layoutX="59.0" layoutY="31.0" prefWidth="150.0" />
      <Button fx:id="submitpdf" layoutX="563.0" layoutY="251.0" mnemonicParsing="false" onAction="#okpdf" text="pdf" />
      <BarChart fx:id="OffersChart" layoutX="23.0" layoutY="294.0" prefHeight="279.0" prefWidth="503.0" title="priority of user stories/project">
         <xAxis>
            <CategoryAxis label="priorities" side="BOTTOM" fx:id="x" />
         </xAxis>
         <yAxis>
            <NumberAxis fx:id="y" label="number of user stories" side="LEFT" />
         </yAxis>
      </BarChart>
      <Button fx:id="ba" layoutX="597.0" layoutY="374.0" mnemonicParsing="false" onAction="#back" text="back" />
   </children>
</AnchorPane>
